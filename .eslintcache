hat';\nimport StorefrontIcon from '@material-ui/icons/Storefront';\nimport VideoLibraryIcon from '@material-ui/icons/VideoLibrary';\nimport { useStateValue } from './.StateProvider';\n\nfunction Sidebar() {\n    const [{user}, dispatch] = useStateValue();\n\n    return (\n        <div className=\"sidebar\">\n            <SidebarRow src = {user.photoURL} title= {user.displayName}/>\n            <SidebarRow Icon={LocalHospitalIcon} title=\"COVID-19 Information Center\"/>\n            <SidebarRow Icon={EmojiFlagsIcon} title=\"Pages\"/>\n            <SidebarRow Icon={PeopleIcon} title=\"Friends\"/>\n            <SidebarRow Icon={ChatIcon} title=\"Messenger\"/>\n            <SidebarRow Icon={StorefrontIcon} title=\"Marketplace\"/>\n            <SidebarRow Icon={VideoLibraryIcon} title=\"Videos\"/>\n            <SidebarRow/>\n        </div>\n    )\n}\n\nexport default Sidebar\n","/home/mr_robot/code-base-linux/fb-clone/src/SidebarRow.js",[],"/home/mr_robot/code-base-linux/fb-clone/src/Feed.js",[],"/home/mr_robot/code-base-linux/fb-clone/src/StoryReel.js",[],"/home/mr_robot/code-base-linux/fb-clone/src/Story.js",[],["75","76"],"/home/mr_robot/code-base-linux/fb-clone/src/MessageSender.js",["77"],"import { Avatar } from '@material-ui/core'\nimport React, { useState } from 'react'\nimport './MessageSender.css'\nimport { InsertEmoticon, PhotoLibrary,  Videocam } from '@material-ui/icons'\nimport { useStateValue } from './.StateProvider'\nimport db from \"./firebase\";\nimport firebase from \"firebase\";\n\n\nfunction MessageSender() {\n    const [{ user }, dispatch] = useStateValue();\n    const [input, setInput] = useState(\"\");\n    const [imageUrl, setImageUrl] = useState(\"\");\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n\n        db.collection('posts').add({\n            message: input,\n            timestamp: firebase.firestore.FieldValue.serverTimestamp(),\n            profilePic: user.photoURL,\n            username: user.displayName,\n            image: imageUrl,\n         } );\n\n        setInput(\"\");\n        setImageUrl(\"\");\n    }\n    return (\n        <div className=\"messageSender\">\n            <div className=\"messageSender__top\">\n                <Avatar src={user.photoURL}/>\n                <form>\n                    <input value={input} onChange={(e) => setInput(e.target.value)} placeholder = {`What's on your mind, ${user.displayName}?`} type=\"text\"/>\n                    <input value={imageUrl} onChange = {(e) => setImageUrl(e.target.value)} placeholder = \"Image URL (Optional)\" type=\"text\"/>\n                    <button onClick={handleSubmit} type=\"submit\">\n                        Hidden submit\n                    </button>\n                </form>\n            </div>\n            <div className=\"messageSender__bottom\">\n                <div className=\"messageSender__option\">\n                <Videocam style={{color: \"red\"}}/>\n                <h3> Live Video </h3>\n                </div>\n\n                <div className=\"messageSender__option\">\n                <PhotoLibrary style={{color: \"green\"}}/>\n                <h3> Photo/Video </h3>\n                </div>\n\n                <div className=\"messageSender__option\">\n                <InsertEmoticon style={{color: \"orange\"}}/>\n                <h3> Feeling/Activity </h3>\n                </div>\n            </div>\n\n            \n        </div>\n    )\n}\n\nexport default MessageSender\n","/home/mr_robot/code-base-linux/fb-clone/src/Post.js",[],"/home/mr_robot/code-base-linux/fb-clone/src/Widgets.js",["78"],"import React from 'react'\nimport './Widgets.css'\n\nfunction Widgets() {\n    return (\n        <div className=\"widgets\">\n            <iframe src =\"https://www.facebook.com/zuck\" width=\"340\" height=\"100%\" style={{border: \"none\", overflow: \"hidden\"}} scrolling=\"no\" frameborder=\"0\" allowTransparency=\"true\" allow=\"encrypted-media\"/>\n            \n        </div>\n    )\n}\n\nexport default Widgets;\n","/home/mr_robot/code-base-linux/fb-clone/src/Login.js",["79"],"import { Button } from '@material-ui/core'\nimport React from 'react'\nimport { useStateValue } from './.StateProvider';\nimport { auth, provider } from './firebase'\nimport \"./Login.css\"\nimport { actionTypes } from './reducer';\n\nfunction Login() {\n    const [state, dispatch] = useStateValue();\n\n    const signIn = () => {\n        //clever sign-in\n        auth.signInWithPopup(provider).then((result) => {\n            dispatch({type: actionTypes.SET_USER, \n                        user: result.user,})\n        });\n    };\n\n    return (\n        <div className=\"login\">\n            <div className=\"login__logo\">\n                <img src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/a/a7/Facebook_f_Logo_%28with_gradient%29.svg/1024px-Facebook_f_Logo_%28with_gradient%29.svg.png\" alt=\"\"/>\n            </div>\n            <div className=\"login__company\">\n            <img src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/0/09/Facebook%2C_Inc._Logo_2019.png/1600px-Facebook%2C_Inc._Logo_2019.png\" alt=\"\"/>\n            </div>\n            <Button type=\"submit\" onClick={signIn}>\n                Sign In\n            </Button>\n        </div>\n    )\n}\n\nexport default Login;\n","/home/mr_robot/code-base-linux/fb-clone/src/firebase.js",[],"/home/mr_robot/code-base-linux/fb-clone/src/reducer.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":12,"column":19,"nodeType":"86","messageId":"87","endLine":12,"endColumn":27},{"ruleId":"84","severity":1,"message":"85","line":18,"column":19,"nodeType":"86","messageId":"87","endLine":18,"endColumn":27},{"ruleId":"84","severity":1,"message":"85","line":13,"column":20,"nodeType":"86","messageId":"87","endLine":13,"endColumn":28},{"ruleId":"80","replacedBy":"88"},{"ruleId":"82","replacedBy":"89"},{"ruleId":"84","severity":1,"message":"85","l